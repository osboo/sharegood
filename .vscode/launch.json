{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": { "PYTHONPATH": "${workspaceRoot}/mybot"}
    },
    {
      "name": "Attach to Python Functions",
      "type": "python",
      "request": "attach",
      "port": 9091,
      "preLaunchTask": "func: host start",
      "justMyCode": false
    },
    {
      "name": "Debug Unit Test",
      "type": "python",
      "request": "test",
      "justMyCode": false,
    },
  ]
}